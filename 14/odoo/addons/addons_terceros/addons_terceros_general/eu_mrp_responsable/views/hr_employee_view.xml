<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="hr_employee_form_view_romana" model="ir.ui.view">
            <field name="name">hr.employee.form.view.romana</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='parent_id']" position="after">
                    <field name="reponsable_quality" />    
                    <field name="reponsable_production" />    
                    <field name="reponsable_pcp" />    
                    <field name="reponsable_romana" />        
                </xpath>
            </field>
        </record>
    </data>
</odoo>
