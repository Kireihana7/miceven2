<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="quality_alert_inherit_form" model="ir.ui.view">
            <field name="name">quality_alert_inherit_form</field>
            <field name="model">quality.alert</field>
            <field name="inherit_id" ref="quality_control.quality_alert_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group/group/field[@name='product_tmpl_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
                <xpath expr="//group/group/field[@name='product_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
                <xpath expr="//group/group/field[@name='lot_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>

                <xpath expr="//group/group/field[@name='team_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
                <xpath expr="//group/group/field[@name='user_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
                <xpath expr="//group/group/field[@name='tag_ids']" position="attributes">
