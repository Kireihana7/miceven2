        </record>

        <record id="quality_alert_inherit_form_2" model="ir.ui.view">
            <field name="name">quality_alert_inherit_form_2</field>
            <field name="model">quality.alert</field>
            <field name="inherit_id" ref="quality_mrp_workorder.quality_alert_view_form_inherit_mrp"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='workcenter_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>