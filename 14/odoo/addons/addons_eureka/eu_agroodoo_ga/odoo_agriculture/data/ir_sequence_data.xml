<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
		<record id="seq_farmer_cropping_request" model="ir.sequence">
			<field name="name">Farmer Cropping Request</field>
			<field name="code">farmer.cropping.request</field>
			<field name="prefix">CROP/REQ/00000</field>
		</record>

		<record id="seq_agriculture_cost_sheet" model="ir.sequence">
			<field name="name">Workday Planning</field>
			<field name="code">agriculture.cost.sheet</field>
			<field name="prefix">WORKDAY-PLANNING/00000</field>
		</record>		

		<record id="seq_project_crop_request" model="ir.sequence">
			<field name="name">Project Crop Request</field>
			<field name="code">project.project</field>
			<field name="prefix">CROP/REQ/00000</field>
		</record>
		
		
		<record id="seq_equipment_reservation" model="ir.sequence">
			<field name="name">Agricultural Equipment</field>
			<field name="code">equipment.reservation</field>
			<field name="prefix">ER</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>			
		</record>

		<record id="seq_labour_management" model="ir.sequence">
			<field name="name">Labour Management</field>
			<field name="code">labour.management</field>
			<field name="prefix">LB</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>			
		</record>		

		<record id="seq_crop_material_management" model="ir.sequence">
			<field name="name">Crop Material Management</field>
			<field name="code">crop.material.management</field>
			<field name="prefix">CPM</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>			
		</record>					
		
		<!-- ============ Fincas, Parcelas y Tablones ============ -->
		<record id="seq_agriculture_fincas_management" model="ir.sequence">
			<field name="name">Farms</field>
			<field name="code">agriculture.fincas</field>
			<field name="prefix">FINCA/00000</field>
		</record>	
		
		<record id="seq_agriculture_parcels_management" model="ir.sequence">
			<field name="name">Parcels</field>
			<field name="code">agriculture.parcelas</field>
			<field name="prefix">PARCELA/00000</field>
		</record>	
		
		<record id="seq_agriculture_tablon_management" model="ir.sequence">
			<field name="name">Planks</field>
			<field name="code">agriculture.tablon</field>
			<field name="prefix">TABLON/00000</field>
		</record>			

	</data>
</odoo>
