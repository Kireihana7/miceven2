<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    
        <record id="sale_commission_sequence" model="ir.sequence">
            <field name="name">Sale Commission Sequence</field>
            <field name="code">sale.commision.seq</field>
            <field name="prefix">SC</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sale_commission_payment_sequence" model="ir.sequence">
            <field name="name">Sale Commission Payment Sequence</field>
            <field name="code">sale.commision.payment.seq</field>
            <field name="prefix">CP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sale_commission_full_sequence" model="ir.sequence">
            <field name="name">Sale Commission Full Sequence</field>
            <field name="code">sale.commision.full.seq</field>
            <field name="prefix">LC</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
