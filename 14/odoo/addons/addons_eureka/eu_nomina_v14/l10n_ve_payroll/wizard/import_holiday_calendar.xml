<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_import_holiday_calendar_wizard_form" model="ir.ui.view">
        <field name="name">Import Holiday Calendar Wizard</field>
        <field name="model">import.holiday.calendar.wizard</field>
        <field name="arch" type="xml">
            <form string="Imprimir">
                <group>
                    <field name="archivo" />
                    <field name="delimiter" required="1"/>
                </group>
                <footer>
                    <button
                        name="create_entries"
                        string="Crear"
                        type="object"
                        icon="fa-cog fa-spin"
                        class="btn-primary"
                        
                    />
                    
                    <button string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
    
    
        
</odoo>
