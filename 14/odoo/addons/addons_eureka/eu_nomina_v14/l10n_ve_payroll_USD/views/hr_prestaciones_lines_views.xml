<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
    
        <record id="hr_prestaciones_employee_line_view_tree" model="ir.ui.view">
            <field name="name">hr.prestaciones.employee.line</field>
            <field name="model">hr.prestaciones.employee.line</field>
            <field name="inherit_id" ref="l10n_ve_payroll.hr_prestaciones_employee_line_view_tree"/>
            <field name="arch" type="xml">
                    <xpath expr="//field[@name='tasa_activa']" position="after">
                        <field name="currency_rate_date" />
                    </xpath>
                    <xpath expr="//field[@name='prestaciones_mes']" position="after">
                        <field name="prestaciones_mes_ref" />
                    </xpath>
                    <xpath expr="//field[@name='anticipos_otorga']" position="after">
                        <field name="anticipos_otorga_ref" />
                    </xpath>
                    <xpath expr="//field[@name='prestaciones_acu']" position="after">
                        <field name="prestaciones_acu_ref" />
                    </xpath>
                    <xpath expr="//field[@name='intereses']" position="after">
                        <field name="intereses_ref" />
                    </xpath>
                    <xpath expr="//field[@name='intereses_acum']" position="after">
                        <field name="intereses_acum_ref" />
                    </xpath>
                    <xpath expr="//field[@name='presta_e_inte']" position="after">
                        <field name="presta_e_inte_ref" />
                    </xpath>

            </field>
        </record>
      
    </data>
</odoo>
