<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_picking_eu_report_picking_modified" model="ir.ui.view">
        <field name="name">stock.picking.view.form.inherit.check.conformity</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='origin']" position="after">
                <field name="check_conformity"  style="color:blue;"/>
            </xpath>
        </field>
    </record>

</odoo>
