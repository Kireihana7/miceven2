<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="quality_point_inherit_form" model="ir.ui.view">
            <field name="name">quality_point_inherit_form</field>
            <field name="model">quality.point</field>
            <field name="inherit_id" ref="quality.quality_point_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group/group/field[@name='product_ids']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
                <!-- <xpath expr="//group/group/field[@name='product_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath> -->
                <xpath expr="//group/group/field[@name='picking_type_ids']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
                <xpath expr="//group/group/field[@name='company_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
                <xpath expr="//group/group/field[@name='user_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>

            </field>
        </record>
    </data>
</odoo>