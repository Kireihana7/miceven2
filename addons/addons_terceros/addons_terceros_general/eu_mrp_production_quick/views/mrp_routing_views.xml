<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="mrp_routing_workcenter_inherit_form_view" model="ir.ui.view">
            <field name="name">mrp_routing_workcenter_inherit_form_view</field>
            <field name="model">mrp.routing.workcenter</field>
            <field name="inherit_id" ref="mrp.mrp_routing_workcenter_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='workcenter_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
                <xpath expr="//field[@name='company_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>