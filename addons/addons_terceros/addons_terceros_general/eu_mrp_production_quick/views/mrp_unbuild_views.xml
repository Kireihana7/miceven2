<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="mrp_production_inherit_views" model="ir.ui.view">
            <field name="name">mrp_unbuild_form_viws</field>
            <field name="model">mrp.unbuild</field>
            <field name="inherit_id" ref="mrp.mrp_unbuild_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
                <xpath expr="//field[@name='bom_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
                <xpath expr="//field[@name='mo_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
                <xpath expr="//field[@name='lot_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
                <xpath expr="//field[@name='company_id']" position="attributes">
                    <attribute name="options">{"no_open": False, "no_create_edit": True, "no_quick_create": True, "no_create": True}</attribute>
                </xpath>
                
            </field>
        </record>
    </data>
</odoo>