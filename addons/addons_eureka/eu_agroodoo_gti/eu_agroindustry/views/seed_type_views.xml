<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="seed_type_view_tree" model="ir.ui.view">
        <field name="name">seed.type.view.tree</field>
        <field name="model">seed.type</field>
        <field name="arch" type="xml">
            <tree string="Tipos de semilla" editable="bottom">
                <field name="name" required="1" />
            </tree>
        </field>
    </record>

    <record id="action_open_seed_type_view_tree" model="ir.actions.act_window">
        <field name="name">Tipo de semilla</field>
        <field name="res_model">seed.type</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>
