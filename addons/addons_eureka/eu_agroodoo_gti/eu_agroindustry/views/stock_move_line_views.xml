<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_move_line_view_tree_inherit" model="ir.ui.view">
        <field name="name">stock.move.line.view.tree.inherit</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_move_line_tree"/>
        <field name="arch" type="xml">
            <field name="product_id" position="after">
                <field name="seed_type_id" readonly="1" />
            </field>
        </field>
    </record>

    <record id="stock_move_line_view_form_inherit" model="ir.ui.view">
        <field name="name">stock.move.line.view.form.inherit</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_move_line_form"/>
        <field name="arch" type="xml">
            <field name="product_id" position="after">
                <field name="seed_type_id" readonly="1" />
            </field>
        </field>
    </record>

</odoo>
