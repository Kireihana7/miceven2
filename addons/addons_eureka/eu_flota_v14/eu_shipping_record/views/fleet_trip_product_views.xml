<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="fleet_trip_product_view_tree" model="ir.ui.view">
        <field name="name">fleet.trip.product.view.tree</field>
        <field name="model">fleet.trip.product</field>
        <field name="arch" type="xml">
            <tree string="Productos de viajes" editable="bottom">
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="fleet_trip_product_view_form" model="ir.ui.view">
        <field name="name">fleet.trip.product.view.form</field>
        <field name="model">fleet.trip.product</field>
        <field name="arch" type="xml">
            <form string="Productos de viajes">
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
